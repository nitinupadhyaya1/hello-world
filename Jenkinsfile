pipeline  {
	agent any 
			stages {
				stage('date') {
					steps {
						sh 'date'
							}
								}
				stage('ps')   {
					steps {
						sh 'ps'
							}
								}
				stage('pwd') {
					steps {
						sh 'pwd'
							}
								}
				stage('ls -l')
					{
						steps {
							sh 'ls -l'
								}
					}
					}
				
		}
